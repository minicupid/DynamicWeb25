import './RecipeCard.css';

const InstructionsList = (props) => {
    const {Data} = props;
    return <div className = "InstructionsList"> 
    <h3 className = "list_title"> instructions </h3>
    <ol>
        {Data.map((item, index) => {
            return (
            <li key = {index} className = "list_item"> 
                {item}
            </li>)}
        )}
    </ol>
     </div>
}

export default InstructionsList;